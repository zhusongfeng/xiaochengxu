.device-detail {
  min-height: 100vh;
  background: #f5f5f5;
}

.detail-header {
  position: relative;
  padding: 33rpx 0 31rpx 36rpx;
  background: #fff;
}

.detail-header .hotel-name {
  margin-bottom: 22rpx;
}

.detail-header .position-block {
  margin-bottom: 24rpx;
}

.position-block .device-position {
  display: flex;
  width: 340rpx;
}

.position-block .pos-content {
  white-space: nowrap;
  text-overflow: ellipsis;
  /* overflow: hidden; */
  font-size:40rpx;
}

.position-block .edit {
  color: #66f;
  margin-left: 10rpx;
  margin-top:8rpx;
  font-size: 24rpx;
}

.position-block .position-input {
  width: 350rpx;
  padding-left: 20rpx;
  border: 1rpx solid #eee;
  border-radius: 10rpx;
  max-height: 1.2em;
}

.detail-header .device-status {
  margin-bottom: 23rpx;
}

.detail-header .device-status image:not(:last-child) {
  margin-right: 10rpx;
}

.detail-header .device-status .icon-wifi {
  width: 28rpx;
  height: 22rpx;
}

.detail-header .device-status .icon-temperature {
  width: 22rpx;
  height: 23rpx;
}

.detail-header .menu {
  position: absolute;
  top: 48rpx;
  right: 61rpx;
  width: 40rpx;
  height: 28rpx;
}

.detail-header .menu .menu-btn {
  width: 100%;
  height: 100%;
  padding: 20rpx;
}

.menu .dropbar {
  position: absolute;
  top: 20rpx;
  left: -200rpx;
  box-sizing: border-box;
  width: 188rpx;
  height: 0;
  padding: 12rpx 0 10rpx;
  border-radius: 10rpx;
  background-color: #fff;
  box-shadow: 0 6rpx 10rpx 7rpx #f3f3f3;
  z-index: 5;
  overflow: hidden;
  opacity: 0;
  transition: all 0.2s ease;
}

.menu.open .dropbar {
  height: 254rpx;
  opacity: 1;
}

.menu .dropbar .menu-item {
  display: flex;
  padding: 15rpx 20rpx;
}

.menu-item .menu-item-icon {
  margin-right: 20rpx;
}

.menu-item .menu-item-icon1 {
  width: 25rpx;
  height: 26rpx;
}

.menu-item .menu-item-icon2 {
  width: 26rpx;
  height: 28rpx;
}

.menu-item .menu-item-icon {
  width: 28rpx;
  height: 28rpx;
}

.menu-item .menu-item-icon {
  width: 28rpx;
  height: 28rpx;
}

.passage-list .passage {
  display: flex;
  padding: 25rpx 35rpx;
  border-top: 1rpx solid #e5e5e5;
  border-bottom: 1rpx solid #e5e5e5;
  background-color: #fff;
  position: relative;
}

.passage-list .passage:not(:last-child) {
  margin-bottom: 20rpx;
}

.image-wrapper {
  height: calc(100%-50);
  display: flex;
  align-items: center;
  position: relative;
}

.change_mask {
  position: absolute;
  left: 0;
  top: 0;
  width: 130rpx;
  height: 130rpx;
  text-align: center;
  line-height: 136rpx;
  background: rgba(0, 0, 0, 0.1);
  color: #fff;
}

.passage .thumbnail {
  width: 130rpx;
  height: 130rpx;
  margin-right: 39rpx;
  border-radius: 10rpx;
  background: rgba(153, 153, 153, 0.1);
}

.passage .passage-body {
  width: 260rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  position:relative;
}
.passage .passage-body .expType{
  color:#f00;
  position:absolute;
  top:0;
  right:0;
}
.passage-body .product-name {
  margin-top: 5rpx;
}

.passage-body .btn-group {
  margin-top: 5rpx;
  position: absolute;
  bottom: 31;
  left: 199;
}

.passage-body .btn-group .btn {
  display: inline-block;
  padding: 8rpx 22rpx;
  font-size: 24rpx;
  line-height: 1em;
  color: #333;
  background-color: #fff;
  margin-top: 10rpx;
}

.btn-group .btn.normal {
  color: #00d10d;
}

.btn-group .btn.lack {
  color: #ff5050;
}

.passage-body .btn-group .btn:not(:last-child) {
  margin-right: 24rpx;
}

.passage-body .btn-group .btn::after {
  border: 1rpx solid #888;
}

.passage-aside {
  text-align: right;
  flex: 1;
}

.passage-aside .tips {
  color: #ff5050;
  margin-top: 6rpx;
}

.tips text {
  margin-left: 10rpx;
}

.passage-aside .normal {
  color: #00d10d;
}

.details-dialog .title {
  color: #ff5050;
  font-size: 26rpx;
  text-align: center;
}

.details-dialog .body .icon-warning {
  display: block;
  width: 176rpx;
  height: 126rpx;
  margin: 20rpx auto;
}

.details-dialog .body .message {
  margin-top: 33rpx;
  font-size: 30rpx;
  text-align: center;
}

.details-dialog .body .message .operator {
  display: inline;
  color: #ff5050;
}

.buttons {
  text-align: center;
}

.buttons .btn {
  display: inline-block;
  width: 150rpx;
  height: 50rpx;
  line-height: 50rpx;
  font-size: 24rpx;
}

.buttons .btn:last-child {
  margin-left: 24rpx;
}

.caution-dialog {
  font-size:40rpx;
}

.caution-dialog .title {
  text-align: center;
}

.caution-dialog .body {
  padding: 0 50rpx 0;
  margin: 30rpx auto 0;
}

.body .radio-item {
  display: inline-block;
  font-weight: 600;
/*  opacity: 0.4;*/
/*  margin-bottom: 40rpx;*/
  color:#666;
}

.body .radio-item.active {
  opacity: 1;
}

.radio-item .radio, .radio-item .content {
  display: inline-block;
}

.radio-item .radio {
  margin-right: 10rpx;
}

.radio .wx-radio-input {
  width: 30rpx;
  height: 30rpx;
}

.radio-item .caution {
  padding: 10rpx 0 0 55rpx;
}

.radio-item .content .w4, .radio-item .content .w2 {
  display: inline-block;
  border: 1rpx solid #abcdef;
  height: 1.3em;
  min-height: 1.3em;
  line-height: 1.3em;
  vertical-align: middle;
  text-align: center;
  margin: 5rpx;
}

.radio-item .content .w4 {
  width: 100rpx;
}

.radio-item .content .w2 {
  width: 60rpx;
}

.overdue-date {
  padding-left: 55rpx;
  color: #ff5050;
}

.GoogsDialog-wrapper {
  display: flex;
  justify-content: center;
  align-items: center;
  position: fixed;
  top: 0;
  left: 0;
  width: 100vw;
  height: 100vh;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 999;
}

.GoogsDialog {
  position: relative;
  background-color: #fff;
  box-sizing: border-box;
  width: 90%;
  min-height: 30vh;
  border-radius: 10rpx;
}

.GoogsDialog-header {
  color: #ff5050;
  font-size: 26rpx;
  text-align: center;
  padding: 8rpx;
}

.GoogsDialog-close {
  position: absolute;
  right: -10rpx;
  top: 0;
  padding: 0rpx 20rpx;
}

.GoogsDialog-body {
  width: 90%;
  margin: 0 auto;
  font-size: 28rpx;
}

.GoogsDialog-body input {
  width: 96%;
  margin: 0 auto;
  display: block;
}

.goods_list {
  max-height: 240rpx;
  overflow-y: auto;
}

.goods_list>view, .goods_header {
  display: flex;
  display: -webkit-flex;
  justify-content: center;
  text-align: center;
  flex-wrap: wrap;
}

.goods_list>view view, .goods_header>view {
  width: 40%;
  height: 60rpx;
  line-height: 60rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.goods_list>view :nth-child(2), .goods_header :nth-child(2) {
  width: 30%;
}

.goods_list>view :nth-child(3), .goods_header :nth-child(3) {
  width: 30%;
}

.goods_list button {
  height: 50rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  width: 20% !important;
}

.hatchInput {
  display: inline-block;
  border: 1rpx solid #333;
  height: 1.3em;
  min-height: 1.3em;
  line-height: 1.3em;
  vertical-align: middle;
  text-align: center;
  margin: 5rpx;
  width: 305rpx;
}
.replenishmentAction{
  width:190rpx;
  height:76rpx;
