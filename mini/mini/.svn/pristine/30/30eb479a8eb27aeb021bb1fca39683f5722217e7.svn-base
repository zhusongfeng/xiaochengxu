<view class="container1">
  <view class='sdr-tableviewcell'>
    <view class="sdr-tableviewcell_label">输入密码</view>
    <view class="mobile-box-flex-1">
      <input class='sdr-tableviewcell_input' type='password' placeholder="请设置您的书堆密码"
    bindinput ="passwordInput"></input>
    </view>
  </view>
  <view class='sdr-tableviewcell'>
    <view class="sdr-tableviewcell_label">确认密码</view>
    <view class="mobile-box-flex-1">
      <input class='sdr-tableviewcell_input' type='password' placeholder="请再次输入您的书堆密码"
    bindinput ="password2Input"></input>
    </view>
  </view>
  <view class='big_bt' bindtap='subInfo'>提交</view>
</view>