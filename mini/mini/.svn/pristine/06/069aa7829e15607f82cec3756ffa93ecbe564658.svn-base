// pages/component/auditFailure/auditFailure.js
const app = getApp()
Page({

  /**
   * 页面的初始数据
   */
  data: {
    name: '',
    idcard: '',
    phone: '',
    address: '',
    hiddenLoading: false,
    reasonList: '',
    nextTo: '',
  },
  // 拨打电话
  calling: function () {
    wx.makePhoneCall({
      phoneNumber: '400-128-0528',
      success: function () {
        console.log(11111111);
      },
      fail: function () {
        console.log(222222);
      }
    })
  },
  /**
   * 生命周期函数--监听页面加载
   */
  onLoad: function (options) {
    this.setData({
      hiddenLoading: false,
      reasonList: options.reasonList,
      nextTo: options.errorKey,
    })
    var _this = this;
    wx.request({
      url: app.globalData.server + '/api/v4/ecard/ecardOrderInfo',
      data: {
        openid: app.globalData.openid
      },
      success: function (res) {
        _this.setData({
          hiddenLoading: true,
          name: res.data.name,
          cardno: res.data.cardno,
          idcard: res.data.idno,
          phone: res.data.mobile,
          address: res.data.address,
        })
      }
    })
  },
  // 重新提交
  nextTo: function() {
    var nextTo = this.data.nextTo; 
    if (nextTo == 'subPayNotpass') {
      wx.redirectTo({
        url: "../payDeposit/payDeposit"
      })
    }else {
      wx.redirectTo({
        url: "../onlineCertificate/onlineCertificate"
      })
    }
  },

  /**
   * 生命周期函数--监听页面初次渲染完成
   */
  onReady: function () {
  
  },

  /**
   * 生命周期函数--监听页面显示
   */
  onShow: function () {
  
  },

  /**
   * 生命周期函数--监听页面隐藏
   */
  onHide: function () {
  
  },

  /**
   * 生命周期函数--监听页面卸载
   */
  onUnload: function () {
  
  },

  /**
   * 页面相关事件处理函数--监听用户下拉动作
   */
  onPullDownRefresh: function () {
  
  },

  /**
   * 页面上拉触底事件的处理函数
   */
  onReachBottom: function () {
  
  }
})