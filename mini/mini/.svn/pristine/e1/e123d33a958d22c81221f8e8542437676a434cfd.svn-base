.item{
  height: 100rpx;
  line-height: 100rpx;
  border-bottom: 2rpx solid #ebeced;
  font-size: 32rpx;
}
.word{
  color: gray;
}
.item-address{
  height:auto;
  line-height:50rpx;
  padding-top: 25rpx;
  padding-bottom: 25rpx;
}
.swiper-item{
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    font-size: 32rpx;
    border-bottom: 2rpx solid #ebeced;
}
.history-item{
    padding-bottom: 10rpx;
}
.swiper-item-title{
