body{
  background-color: #f5f5f5;
}
.cardList{
  width: 100%;
}
.cardItem{
  background-color: #ffffff;
  box-shadow: 10rpx 10rpx 5rpx ghostwhite;
  padding: 40rpx;
  margin-top: 20rpx;
  border-left: 2rpx solid #f5f5f5;
  border-top: 2rpx solid #f5f5f5;
}
.cardItem:first-child{
  margin-top: 5rpx;
}
.bookcardNum{
  font-size: 32rpx;
}
.lib{
  font-size: 32rpx;
  color: gray;
  margin-top: 10rpx;
}
.bt{
  margin-top:40rpx;
  float: right;
  font-size: 30rpx;
  color: gray;
}
.unbind_bt{
  margin-left: 30rpx;
}
.default{
  color: #509AFF;
}
.small_bt{
  width:525rpx;
}
.small_bt1{
  width:525rpx;
  height:65rpx;
  line-height: 65rpx;
  border-radius: 10rpx;
  background: white;
  border: 1px solid #dddddd;
  margin:50rpx auto;
  font-size: 33rpx;
  text-align: center;

}