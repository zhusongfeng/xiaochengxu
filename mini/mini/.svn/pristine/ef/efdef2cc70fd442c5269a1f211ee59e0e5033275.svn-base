@import "../../../common/weui.wxss";

.sdr-tableviewcell{
  display: flex;
  -webkit-align-items:center;
  font-size: 32rpx;
  border-bottom: 2rpx solid #ebeced;
}
.sdr-tableviewcell_label{
  width: 180rpx;
  height: 120rpx;
  line-height: 120rpx;
}
.mobile-box-flex-1{
