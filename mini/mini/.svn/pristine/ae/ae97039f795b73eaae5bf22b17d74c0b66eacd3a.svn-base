.container1{
  padding: 10rpx;
}
.head{
  background-color: #509AFF;
  color: #fff;
  padding: 40rpx;
  height: 200rpx;
  font-size: 32rpx;
  border-radius: 10rpx;
  box-shadow: 5rpx 5rpx 5rpx gray;
  line-height: 200rpx;
  display: flex;
}
.headpic{
  background-color: gainsboro;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50rpx;
  line-height: 100rpx;
  margin-top: 50rpx;
  float: left;
}
.loginhead{
  margin-left: 40rpx;
  float: left;
}
.cancel,.setting{
  height: 100rpx;
  font-size: 30rpx;
  color: gray;
  line-height: 100rpx;
  float: right;
  margin-top: 50rpx;
  margin-left: 60rpx;
}
.flex-item {  
  display: flex;
  font-size: 32rpx;
  height: 110rpx;
  line-height: 110rpx;
  border-bottom: 2rpx solid #ebeced;
  padding-left:10rpx;
  padding-right:10rpx;
}
.news{
  margin-top: 20rpx;
}
.itemPic{
  width: 60rpx;
  height: 60rpx;
  margin-top: 25rpx;
}
.itemName{
  margin-left: 20rpx;
}
.backPic{
  width: 30rpx;
  height: 30rpx;
  margin-left: auto;
  margin-top: 40rpx;
}
.onlineService{
  width: 100rpx;
  height: 100rpx;
}