@import "../../common/weui.wxss";

.picker{
  padding: 19rpx 26rpx;
  background-color: #FFFFFF;
}

.sdr-tableviewcell{
  display: flex;
  -webkit-align-items:center;
  font-size: 30rpx;
  border-bottom: 2rpx solid #ebeced;
}
.sdr-tableviewcell_label{
  width: 160rpx;
  height: 100rpx;
  line-height: 100rpx;
}
.mobile-box-flex-1{
  flex: 1;
}
.sdr-tableviewcell_input{
  padding: 0 20rpx;
  height: 100rpx;
  line-height:100rpx;
  box-sizing: border-box;
  width:100%;
}
.organization{
  border-top: 2rpx solid #ebeced;
  border-bottom: 10rpx solid #ebeced;
}
.address,.education{
  border-bottom: 10rpx solid #ebeced;
}
.prompt{
  font-size: 28rpx;
  color: #333;
  margin-top: 30rpx;
}