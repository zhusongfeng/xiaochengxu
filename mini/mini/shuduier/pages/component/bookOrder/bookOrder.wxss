.address{
  width: 100%;
  height: 160rpx;
  display: flex;
  flex-flow:row wrap;
  font-size: 32rpx;
  border-bottom: 10rpx solid #ebeced;
  display: -webkit-box;
  -webkit-box-align: center;
  -webkit-box-pack: center;
}
.icon_address{
  width: 10%;
}
.icon_address image{
  width: 60rpx;
  height: 60rpx;
}
.icon_back image{
  width: 30rpx;
  height: 30rpx;
}
.icon1{
  width: 90rpx;
}
.address_info{
  width: 80%;
}
.icon_back{
  width: 10%;
}
.bookInfo{
  display: flex;
  width: 100%;
  padding-top: 32rpx;
  border-bottom: 2rpx solid #ebeced;
}
.bookPic{
  width: 30%;
}
.bookImage{
  width:160rpx;
  height: 180rpx;
}
.bookText{
  width: 70%;
  font-size: 30rpx;
}
.bookAuthor{
  color: gray;
}
.bookPrice{
  color: #509AFF;
}
bookTitle,.bookAuthor,.bookPrice,bookCom{
  margin-top: 10rpx;
}
.message{
  width: 100%;
  font-size: 30rpx;
  border-bottom: 10rpx solid #ebeced;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.returnLib{
  width: 100%;
  border-bottom: 2rpx solid #ebeced;
  font-size: 32rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}
.bookDescribe{
  width: 100%;
  font-size: 32rpx;
}
.bookIntro{
  text-indent: 2em;
  line-height: 50rpx;
}
.contentInfo{
  margin-top: 20rpx;
}
.contentDetails{
  color: gray;
  font-size: 30rpx;
  margin-top: 20rpx;
}